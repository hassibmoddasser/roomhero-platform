{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": "warn",
    "quotes": [
      "error",
      "double"
    ],
    "linebreak-style": 1,
    "react/react-in-jsx-scope": "off",
    "no-duplicate-imports": "warn",
    "react/prop-types": "off",
    "no-unused-vars": "off"
  }
}